[
  {
    "Label": "Share finished torrents updated",
    "Condition": {
      "$type": "ChatBeet.Queuing.Rules.TitleCondition, ChatBeet.Queuing",
      "Match": "^Finished Torrent \"(.*)\""
    },
    "TargetChannel": "#ðŸ¥•",
    "Output": {
      "$type": "ChatBeet.Queuing.Rules.TemplatedOutputGenerator, ChatBeet.Queuing",
      "Template": "{{Title}}"
    },
    "Type": 1,
    "Pipes": [
      {
        "$type": "ChatBeet.Queuing.Rules.RegexPipe, ChatBeet.Queuing",
        "Pattern": "^Finished Torrent \"(.*)\"",
        "OutputTemplate": "Torrent finished downloading: $1"
      }
    ]
  },
  {
    "Label": "Say hello",
    "Condition": {
      "$type": "ChatBeet.Queuing.Rules.AndCondition, ChatBeet.Queuing",
      "Conditions": [
        {
          "$type": "ChatBeet.Queuing.Rules.TitleCondition, ChatBeet.Queuing",
          "Match": "^PRIVMSG",
          "IgnoreCase": true
        },
        {
          "$type": "ChatBeet.Queuing.Rules.SourceCondition, ChatBeet.Queuing",
          "Match": "^irc:(.*)",
          "IgnoreCase": false
        },
        {
          "$type": "ChatBeet.Queuing.Rules.BodyCondition, ChatBeet.Queuing",
          "Match": "ðŸ¥• hello",
          "IgnoreCase": true
        }
      ]
    },
    "TargetChannel": "#ðŸ¥•",
    "Output": {
      "$type": "ChatBeet.Queuing.Rules.TemplatedOutputGenerator, ChatBeet.Queuing",
      "Template": "{{Source}}"
    },
    "Type": 0,
    "Pipes": [
      {
        "$type": "ChatBeet.Queuing.Rules.RegexPipe, ChatBeet.Queuing",
        "Pattern": "^irc:(.*)",
        "OutputTemplate": "Hello $1! ðŸ™‚"
      }
    ]
  },
  {
    "label": "Stop! Wait a minute",
    "condition": {
      "$type": "ChatBeet.Queuing.Rules.AndCondition, ChatBeet.Queuing",
      "conditions": [
        {
          "$type": "ChatBeet.Queuing.Rules.TitleCondition, ChatBeet.Queuing",
          "match": "^PRIVMSG",
          "ignoreCase": true
        },
        {
          "$type": "ChatBeet.Queuing.Rules.SourceCondition, ChatBeet.Queuing",
          "match": "^irc:(.*)",
          "ignoreCase": false
        },
        {
          "$type": "ChatBeet.Queuing.Rules.BodyCondition, ChatBeet.Queuing",
          "match": "stop([?!.]+)?",
          "ignoreCase": true
        }
      ]
    },
    "targetChannel": "#ðŸ¥•",
    "type": 0,
    "output": {
      "$type": "ChatBeet.Queuing.Rules.TemplatedOutputGenerator, ChatBeet.Queuing",
      "template": "{{Body}}"
    },
    "pipes": [
      {
        "$type": "ChatBeet.Queuing.Rules.RegexPipe, ChatBeet.Queuing",
        "pattern": "^stop([?!.]+)?",
        "outputTemplate": "Wait a minute$1",
        "ignoreCase": true
      }
    ]
  },
  {
    "label": "Startup",
    "condition": {
      "$type": "ChatBeet.Queuing.Rules.AndCondition, ChatBeet.Queuing",
      "conditions": [
        {
          "$type": "ChatBeet.Queuing.Rules.TargetCondition, ChatBeet.Queuing",
          "match": "system",
          "ignoreCase": true
        },
        {
          "$type": "ChatBeet.Queuing.Rules.SourceCondition, ChatBeet.Queuing",
          "match": "startup",
          "ignoreCase": false
        }
      ]
    },
    "targetChannel": "#ðŸ¥•",
    "type": 0,
    "output": {
      "$type": "ChatBeet.Queuing.Rules.TemplatedOutputGenerator, ChatBeet.Queuing",
      "template": "{{Body}}"
    },
    "pipes": []
  }
]